{
  "name": "@reactless/vite-plugin",
  "version": "0.0.1",
  "description": "Vite plugin for Reactless automatic JSX optimization",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "npm run build -- --watch",
    "test": "vitest"
  },
  "peerDependencies": {
    "vite": "7.3.1"
  },
  "dependencies": {
    "@reactless/core": "workspace:*"
  },
  "devDependencies": {
    "vite": "7.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
